extends layout

block content
    body.ranking-page
        .header-wrapper-woos
            .header-woos
                p#codename
        .ranking-container
            .ranking-selection-box
                h1#selection-title Woos
                p#selection-para People who like you
                .ranking-selection-list
                    - var i =0, j =0;
                    each sample in chatmate
                    .ranking-left
                        .img-photo
                            img.chat_photo(id=JSON.parse(chatmate[0]).codename, src=chatmate.photourl)
                        p.chat-mate=chatmate.codename
                        a#fblinkicon(href="http://facebook.com/"+chatmate.username)
                        a#twlinkicon(href="http://twitter.com/"+chatmate.username)
                .submit-part
                    button#submit_rating(type='button' href="/option")
                    p#btn-woos Go back and mingle some more!
                    input(type="hidden",id="chatm8",value=chatmate)
                    input(type="hidden",id="chatm7",value=chatmate.length))
                    input(type="hidden",id="user1",value=JSON.stringify(user))